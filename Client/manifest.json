{
  "manifest_version": 3,
  "name": "Prompt Enhancer for ChatGPT",
  "version": "0.1.0",
  "description": "Automatically enhances prompts typed into ChatGPT.",
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*"
      ],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": [
    "https://your-backend-domain.com/*"
  ]
}
